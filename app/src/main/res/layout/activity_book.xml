<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Book"
    android:id="@+id/relativeLayout">

    <Button
        android:id="@+id/a"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:textSize="40sp"
        android:textColor="@color/past"
        android:textStyle="bold"
        android:background="@color/blue"
        android:text="A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/b"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:textSize="40sp"
        android:textColor="@color/pink"
        android:textStyle="bold"
        android:background="@color/past"
        android:text="B"
        app:layout_constraintStart_toEndOf="@+id/a"
        app:layout_constraintTop_toTopOf="@+id/a" />

    <Button
        android:id="@+id/c"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:textSize="40sp"
        android:textColor="@color/blue"
        android:textStyle="bold"
        android:background="@color/red"
        android:text="C"
        app:layout_constraintStart_toEndOf="@+id/b"
        app:layout_constraintTop_toTopOf="@+id/b" />

    <Button
        android:id="@+id/d"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:textSize="40sp"
        android:textColor="@color/yelow"
        android:textStyle="bold"
        android:background="@color/green"
        android:text="D"
        app:layout_constraintStart_toEndOf="@+id/c"
        app:layout_constraintTop_toTopOf="@+id/c" />

    <Button
        android:id="@+id/e"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:textSize="40sp"
        android:textColor="@color/red"
        android:textStyle="bold"
        android:background="@color/yelow"
        android:text="E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/d"
        app:layout_constraintTop_toTopOf="@+id/d" />

    <Button
        android:id="@+id/f"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:textSize="40sp"
        android:textColor="@color/past"
        android:textStyle="bold"
        android:background="@color/blue"
        android:text="F"
        app:layout_constraintStart_toStartOf="@+id/a"
        app:layout_constraintTop_toBottomOf="@+id/a" />

    <Button
        android:id="@+id/g"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/pink"
        android:textStyle="bold"
        android:background="@color/past"
        android:text="G"
        app:layout_constraintStart_toStartOf="@+id/b"
        app:layout_constraintTop_toTopOf="@+id/f" />

    <Button
        android:id="@+id/h"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/blue"
        android:textStyle="bold"
        android:background="@color/red"
        android:text="H"
