    <Button
        android:id="@+id/i"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/yelow"
        android:textStyle="bold"
        android:background="@color/green"
        android:text="I"
        app:layout_constraintStart_toStartOf="@+id/d"
        app:layout_constraintTop_toTopOf="@+id/h" />

    <Button
        android:id="@+id/j"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/red"
        android:textStyle="bold"
        android:background="@color/yelow"
        android:text="J"
        app:layout_constraintStart_toStartOf="@+id/e"
        app:layout_constraintTop_toTopOf="@+id/i" />

    <Button
        android:id="@+id/k"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="12dp"
        android:textSize="40sp"
        android:textColor="@color/past"
        android:textStyle="bold"
        android:background="@color/blue"
        android:text="K"
        app:layout_constraintStart_toStartOf="@+id/f"
        app:layout_constraintTop_toBottomOf="@+id/f" />

    <Button
        android:id="@+id/l"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/pink"
        android:textStyle="bold"
        android:background="@color/past"
        android:text="L"
        app:layout_constraintStart_toStartOf="@+id/g"
        app:layout_constraintTop_toTopOf="@+id/k" />

    <Button
        android:id="@+id/m"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/blue"
        android:textStyle="bold"
        android:background="@color/red"
        android:text="M"
        app:layout_constraintStart_toStartOf="@+id/h"
        app:layout_constraintTop_toTopOf="@+id/l" />

    <Button
        android:id="@+id/n"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/yelow"
        android:textStyle="bold"
        android:background="@color/green"
        android:text="N"
        app:layout_constraintStart_toStartOf="@+id/i"
        app:layout_constraintTop_toTopOf="@+id/m" />

    <Button
        android:id="@+id/o"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/red"
        android:textStyle="bold"
        android:background="@color/yelow"
        android:text="O"
        app:layout_constraintStart_toStartOf="@+id/j"
        app:layout_constraintTop_toTopOf="@+id/n" />

    <Button
        android:id="@+id/p"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:textSize="40sp"
        android:textColor="@color/past"
        android:textStyle="bold"
        android:background="@color/blue"
        android:text="P"
        app:layout_constraintStart_toStartOf="@+id/k"
        app:layout_constraintTop_toBottomOf="@+id/k" />

    <Button
        android:id="@+id/q"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/pink"
        android:textStyle="bold"
        android:background="@color/past"
        android:text="Q"
