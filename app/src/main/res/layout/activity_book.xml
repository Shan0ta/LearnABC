        android:textSize="40sp"
        android:textColor="@color/blue"
        android:textStyle="bold"
        android:background="@color/red"
        android:text="R"
        app:layout_constraintStart_toStartOf="@+id/m"
        app:layout_constraintTop_toTopOf="@+id/q" />

    <Button
        android:id="@+id/s"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/yelow"
        android:textStyle="bold"
        android:background="@color/green"
        android:text="S"
        app:layout_constraintStart_toStartOf="@+id/n"
        app:layout_constraintTop_toTopOf="@+id/r" />

    <Button
        android:id="@+id/t"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/red"
        android:textStyle="bold"
        android:background="@color/yelow"
        android:text="T"
        app:layout_constraintStart_toStartOf="@+id/o"
        app:layout_constraintTop_toTopOf="@+id/s" />

    <Button
        android:id="@+id/u"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:textSize="40sp"
        android:textColor="@color/past"
        android:textStyle="bold"
        android:background="@color/blue"
        android:text="U"
        app:layout_constraintStart_toStartOf="@+id/p"
        app:layout_constraintTop_toBottomOf="@+id/p" />

    <Button
        android:id="@+id/v"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/pink"
        android:textStyle="bold"
        android:background="@color/past"
        android:text="V"
        app:layout_constraintStart_toStartOf="@+id/q"
        app:layout_constraintTop_toTopOf="@+id/u" />

    <Button
        android:id="@+id/w"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/blue"
        android:textStyle="bold"
        android:background="@color/red"
        android:text="W"
        app:layout_constraintStart_toStartOf="@+id/r"
        app:layout_constraintTop_toTopOf="@+id/v" />

    <Button
        android:id="@+id/x"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/yelow"
        android:textStyle="bold"
        android:background="@color/green"
        android:text="X"
