        app:layout_constraintTop_toTopOf="@+id/w" />

    <Button
        android:id="@+id/y"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:textSize="40sp"
        android:textColor="@color/red"
        android:textStyle="bold"
        android:background="@color/yelow"
        android:text="Y"
        app:layout_constraintStart_toStartOf="@+id/t"
        app:layout_constraintTop_toTopOf="@+id/x" />

    <Button
        android:id="@+id/z"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:layout_alignBottom="@+id/b"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:textSize="40sp"
        android:textColor="@color/past"
        android:textStyle="bold"
        android:background="@color/blue"
        android:text="Z"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/w"
        app:layout_constraintVertical_bias="0.0" />



